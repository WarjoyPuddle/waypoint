{
  "buildPresets": [
    {
      "configurePreset": "configure",
      "hidden": false,
      "name": "build"
    }
  ],
  "configurePresets": [
    {
      "environment": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": "TRUE"
      },
      "hidden": true,
      "name": "base_export_compile_commands"
    },
    {
      "binaryDir": "${sourceDir}/build___",
      "generator": "Ninja Multi-Config",
      "hidden": false,
      "inherits": [
        "base_export_compile_commands"
      ],
      "name": "configure"
    }
  ],
  "testPresets": [
    {
      "configurePreset": "configure",
      "execution": {
        "noTestsAction": "error",
        "scheduleRandom": true,
        "stopOnFailure": false,
        "timeout": 3600
      },
      "hidden": false,
      "name": "test",
      "output": {
        "outputOnFailure": true,
        "verbosity": "default"
      }
    }
  ],
  "version": 10
}
